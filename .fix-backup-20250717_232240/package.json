{
    "name": "ip-roast-frontend",
    "version": "3.0.0",
    "description": "IP Roast Enterprise - Frontend приложение для тестирования безопасности сети",
    "type": "module",
    "private": true,
    "keywords": [
        "security",
        "network-scanning",
        "vulnerability-assessment",
        "penetration-testing",
        "nmap",
        "security-tools",
        "react",
        "typescript"
    ],
    "author": "IP Roast Development Team",
    "license": "MIT",
    "homepage": "https://github.com/ip-roast/frontend",
    "repository": {
        "type": "git",
        "url": "https://github.com/ip-roast/frontend.git"
    },
    "bugs": {
        "url": "https://github.com/ip-roast/frontend/issues"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    },
    "scripts": {
        "dev": "vite --host 0.0.0.0 --port 3000",
        "build": "tsc && vite build",
        "build:prod": "tsc && vite build --mode production",
        "build:staging": "tsc && vite build --mode staging",
        "build:demo": "tsc && vite build --mode demo",
        "preview": "vite preview --port 3001",
        "serve": "vite preview",
        "type-check": "tsc --noEmit",
        "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix --ignore-path .gitignore",
        "lint:style": "stylelint **/*.{css,scss,sass} --fix",
        "format": "prettier --write src/",
        "format:check": "prettier --check src/",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:e2e": "cypress run",
        "test:e2e:dev": "cypress open",
        "analyze": "vite-bundle-analyzer",
        "clean": "rimraf dist node_modules/.vite",
        "install:clean": "npm run clean && npm install",
        "security:audit": "npm audit --audit-level moderate",
        "security:fix": "npm audit fix",
        "deploy": "npm run build && gh-pages -d dist",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:preview": "vitepress preview docs"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.8.0",
        "react-error-boundary": "^4.0.11",
        "socket.io-client": "^4.8.1",
        "axios": "^1.7.7",
        "chart.js": "^4.5.0",
        "chartjs-adapter-date-fns": "^3.0.0",
        "react-chartjs-2": "^5.2.0",
        "date-fns": "^3.6.0",
        "lodash-es": "^4.17.21",
        "uuid": "^9.0.1",
        "js-yaml": "^4.1.0",
        "file-saver": "^2.0.5",
        "jszip": "^3.10.1",
        "zustand": "^4.4.7",
        "clsx": "^2.0.0",
        "react-hot-toast": "^2.4.1",
        "framer-motion": "^10.16.16",
        "lucide-react": "^0.295.0",
        "d3": "^7.8.5",
        "nprogress": "^0.2.0",
        "monaco-editor": "^0.52.2",
        "highlight.js": "^11.11.1",
        "marked": "^12.0.2",
        "dompurify": "^3.2.6",
        "qrcode": "^1.5.4",
        "copy-to-clipboard": "^3.3.3",
        "hotkeys-js": "^3.13.14",
        "sortablejs": "^1.15.6",
        "fuse.js": "^7.1.0",
        "validator": "^13.15.15",
        "zod": "^3.25.67",
        "@tanstack/react-query": "^5.17.0",
        "@tanstack/react-virtual": "^3.0.1",
        "react-window": "^1.8.8",
        "react-virtualized-auto-sizer": "^1.0.24"
    },
    "devDependencies": {
        "@types/react": "^18.2.43",
        "@types/react-dom": "^18.2.17",
        "@types/node": "^20.19.2",
        "@types/lodash-es": "^4.17.12",
        "@types/uuid": "^9.0.8",
        "@types/js-yaml": "^4.0.9",
        "@types/file-saver": "^2.0.7",
        "@types/nprogress": "^0.2.3",
        "@types/marked": "^6.0.0",
        "@types/dompurify": "^3.2.0",
        "@types/qrcode": "^1.5.5",
        "@types/sortablejs": "^1.15.8",
        "@types/validator": "^13.15.2",
        "@types/d3": "^7.4.3",
        "@types/react-window": "^1.8.8",
        "@vitejs/plugin-react": "^4.2.1",
        "@vitejs/plugin-react-swc": "^3.5.0",
        "typescript": "^5.3.3",
        "vite": "^5.4.19",
        "vite-plugin-checker": "^0.7.2",
        "vite-plugin-pwa": "^0.20.5",
        "vite-plugin-environment": "^1.1.3",
        "vite-bundle-analyzer": "^0.18.0",
        "vite-plugin-compression": "^0.5.1",
        "rollup-plugin-visualizer": "^5.14.0",
        "esbuild": "^0.21.5",
        "@eslint/js": "^8.57.1",
        "eslint": "^8.57.1",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "@typescript-eslint/eslint-plugin": "^6.14.0",
        "@typescript-eslint/parser": "^6.14.0",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-security": "^2.1.1",
        "eslint-plugin-sonarjs": "^0.25.1",
        "prettier": "^3.6.2",
        "stylelint": "^16.21.0",
        "stylelint-config-standard": "^36.0.1",
        "stylelint-config-recess-order": "^4.4.0",
        "@testing-library/react": "^14.1.2",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/user-event": "^14.6.1",
        "vitest": "^1.6.0",
        "@vitest/ui": "^1.6.0",
        "@vitest/coverage-v8": "^1.6.0",
        "jsdom": "^24.1.3",
        "cypress": "^13.17.0",
        "cypress-real-events": "^1.14.0",
        "tailwindcss": "^3.4.0",
        "postcss": "^8.4.47",
        "postcss-preset-env": "^8.4.1",
        "autoprefixer": "^10.4.21",
        "sass": "^1.89.2",
        "rimraf": "^5.0.10",
        "cross-env": "^7.0.3",
        "dotenv": "^16.6.1",
        "workbox-precaching": "^7.3.0",
        "workbox-routing": "^7.3.0",
        "workbox-strategies": "^7.3.0",
        "gh-pages": "^6.1.1",
        "vitepress": "^1.6.3",
        "shiki": "^1.29.2"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead",
        "not ie 11"
    ],
    "postcss": {
        "plugins": {
            "tailwindcss": {},
            "autoprefixer": {},
            "postcss-preset-env": {
                "stage": 1
            }
        }
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{css,scss,sass}": [
            "stylelint --fix"
        ],
        "*.{json,md,yaml,yml}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-push": "npm run test:run"
        }
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "volta": {
        "node": "18.19.0",
        "npm": "10.2.3"
    }
}